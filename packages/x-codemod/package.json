{
  "name": "@mui/x-codemod",
  "version": "7.0.0",
  "bin": "./codemod.js",
  "private": false,
  "author": "MUI Team",
  "description": "Codemod scripts for MUI X.",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "mui-x",
    "codemod",
    "jscodeshift"
  ],
  "scripts": {
    "test": "cd ../../ && cross-env NODE_ENV=test mocha 'packages/x-codemod/**/*.test.ts'",
    "typescript": "tsc -p tsconfig.json",
    "prebuild": "rimraf build",
    "copy-files": "cpy README.md build && cpy package.json build",
    "build": "node ../../scripts/build.mjs node --ignore 'src/types.ts' && pnpm copy-files"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mui/mui-x.git",
    "directory": "packages/x-codemod"
  },
  "license": "MIT",
  "homepage": "https://github.com/mui/mui-x/tree/master/packages/x-codemod",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui-org"
  },
  "dependencies": {
    "@babel/core": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@babel/traverse": "^7.24.6",
    "jscodeshift": "0.13.1",
    "jscodeshift-add-imports": "^1.0.10",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.5",
    "dayjs": "^1.11.11",
    "moment-timezone": "^0.5.45",
    "rimraf": "^5.0.5"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
