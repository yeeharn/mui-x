    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/rest": "^20.0.2",
    "@playwright/test": "^1.43.1",
    "@testing-library/react": "^14.3.1",
    "@types/babel__core": "^7.20.5",
    "@types/chai": "^4.3.16",
    "@types/chai-dom": "^1.11.3",
    "@types/enzyme": "3.10.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.33",
